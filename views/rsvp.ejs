                                                        var hideGroupClass = ((groupClass.type == "other" && strSearch == "") || (groupClass.type == "suggest" && strSearch != ""));

                                                        var searchResultsSectionHtml = "";
                                                        searchResultsSectionHtml += "<li " + (hideGroupClass ? " style='display:none;'" : "") + " data-type='" + groupClass.type + "'><h2 class='header__group--header' style='margin-top: 0px; margin-bottom: 0px; padding: 10px 20px 12px 20px !important;'>" + groupClass.heading + "</h2><ul style='padding-inline-start: 0px !important;' aria-label='" + getSafeAriaLabelTextTrim(groupClass.heading) + "'>";

                                                        if (isEmpty(ariaLiveResultsAccLabel)) {
                                                            ariaLiveResultsAccLabel += 'Found';
                                                        } else {
                                                            ariaLiveResultsAccLabel += ', and';
                                                        }
                                                        ariaLiveResultsAccLabel += ' ' + groupClass.groups.length + ' groups in ' + groupClass.heading;

                                                        groupClass.groups.forEach(function (group) {
                                                            let addOrRemove = (group.favoriteLink.includes('Unstar')) ? 'Remove from ' : 'Add to ';
                                                            let groupFavoriteLinkAriaLabel = addOrRemove + "favorites. " + getSafeAriaLabelTextTrim(group.groupName);
                                                            let groupFavoriteLink = group.favoriteLink.replace(" href=", " aria-label='" + groupFavoriteLinkAriaLabel + "' href=");
